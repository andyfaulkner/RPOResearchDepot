@model IEnumerable<string>

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
    {
        Controller = "Product",
        Action = "List",
        category = link,
        page = 1
    }, 
    new
    {
    @class = "btn btn-default btn-sm"
        + (link == ViewBag.SelectedCategory ? " btn-warning" : "")
})
}
